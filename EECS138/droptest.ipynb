import math

Cd = 0.5
g = 32.2
rho = 2.37E-03

def lbs2slugs(p):
    return p/32.1740

def fps2mph(f):
    return f/5280.0*3600.0

def terminalVelocity(m, A):
    return fps2mph(math.sqrt((2.0*lbs2slugs(m)*g)/(Cd*rho*A)))
  
def printTableHeads():
    print("%-30s\t           \t           \tTerminal      " % "")
    print("%-30s\tWeight (lb)\tArea (ft^3)\tVelocity (mph)" % "Object")
    print("%-30s\t-----------\t-----------\t--------------" % "------")

def printTableEntry (s, m, A):
    v_t = terminalVelocity(m, A)
    print("%-30s\t%-11.3g\t%-11.3g\t%-14.0f" % (s, m, A, v_t))
 
printTableHeads()
printTableEntry("Skydiver in free fall       ", 180.0,  9.0    )
printTableEntry("Skydiver with open parachute", 180.0,  226.0  )
printTableEntry("Baseball (2.88-in. diameter)", 0.32,   0.045  )
printTableEntry("Golf ball (1.65in. diameter)", 0.10,   0.015  )
printTableEntry("Raindrop (0.16-in. diameter)", 7.5E-5, 1.39E-4)
